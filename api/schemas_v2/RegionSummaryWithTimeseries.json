          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "positiveTests": {
          "title": "Positivetests",
          "description": "Cumulative positive test results to date",
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ]
        },
        "negativeTests": {
          "title": "Negativetests",
