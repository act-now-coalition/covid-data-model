          "name": "fips",
          "in": "path",
          "description": "5 Letter County FIPS code",
          "required": true,
          "deprecated": false,
          "allowEmptyValue": false,
          "explode": false,
          "allowReserved": false,
          "schema": {
            "type": "string"
          }
        },
        {
          "name": "apiKey",
          "in": "query",
